<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent">
  
	<TextView android:text="Nome do Programa"
		android:layout_height="wrap_content" android:layout_width="fill_parent"
	/>
	<EditText android:id="@+id/edtNome"
		android:layout_height="wrap_content" android:layout_width="fill_parent"
	/>
	<TextView android:text="Data Inicial"
		android:layout_height="wrap_content" android:layout_width="fill_parent"
	/>
	<EditText android:id="@+id/edtDataInicial"
		android:layout_height="wrap_content" android:layout_width="fill_parent"
	/>
	<TextView android:text="Data Final"
		android:layout_height="wrap_content" android:layout_width="fill_parent"
	/>
	<EditText android:id="@+id/edtDataFinal"
		android:layout_height="wrap_content" android:layout_width="fill_parent"
	/>
	<LinearLayout android:layout_width="wrap_content"
		android:layout_height="fill_parent">
		<Button android:text="Salvar" android:id="@+id/btnSalvar"
			android:layout_height="wrap_content" android:layout_width="wrap_content"
		/>
		<Button android:text="Cancelar" android:id="@+id/btnCancelar"
			android:layout_height="wrap_content" android:layout_width="wrap_content"
		/>
	</LinearLayout>
</LinearLayout>
